  "query": "\n        SELECT \n            id as \"id!\",\n            username as \"username!\",\n            COALESCE(display_name, username) as \"name!\",\n            email as \"email!\",\n            role as \"role!\",\n            CASE WHEN is_active = 1 THEN 'active' ELSE 'inactive' END as \"status!\",\n            datetime('now') as \"last_login!\"\n        FROM users\n        WHERE is_active IN (0, 1)\n        ORDER BY username\n        ",
