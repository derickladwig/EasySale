  "query": "\n        SELECT id as \"id!: i64\", key, value, scope, scope_id, data_type, created_at, updated_at\n        FROM settings\n        WHERE key = ? AND scope = ? AND (scope_id = ? OR scope_id IS NULL)\n        ORDER BY \n            CASE scope\n                WHEN 'user' THEN 1\n                WHEN 'station' THEN 2\n                WHEN 'store' THEN 3\n                WHEN 'global' THEN 4\n            END\n        LIMIT 1\n        ",
